P_sq_DEM <- 24000
yCeil_DEM <- 300
lambda_DEM <- 0
cStar_sq_DEM
beta_DEM <- -cStar_sq_DEM * lambertW0(-cStar_sq_DEM / rCeil_sq_DEM * (1 + lambda_DEM))
yStar_sq_DEM <- yCeil_DEM * exp(-beta_DEM / cStar_sq_DEM)

P_sq_SUP <- 2500
cv_ly <- 0.03
# m_y <- log(150)
# s_y <- cv_ly * m_y
