# Define functions:
# rootfn_cstar <- function(yStar_SUP, yCeil_SUP, cStar_SUP, beta_SUP){
#   slack <- beta_SUP / cStar_SUP + lambertW0(-yStar_SUP / yCeil_SUP * beta_SUP / cStar_SUP)
#   return(slack)
# }
rootfn_cstar <- function(cStar_SUP, yCeil_SUP, beta_SUP, lambda_SUP){
  slack <- beta_SUP / cStar_SUP + lambertW0(-cStar_SUP / (P_SQ_SUP * yCeil_SUP) * (1 + lambda_SUP))
  return(slack)
}
#---------------------------------------------------------
