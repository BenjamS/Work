yStar_sq_DEM <- yCeil_DEM * exp(-beta_DEM / cStar_sq_DEM)
yStar_sq_DEM
P <- 2500
# alpha_x <- 150
# xStar_sq_DEM <- sqrt(P_sq_DEM * yStar_sq_DEM / (1 + lambda_DEM) * alpha_x / P)
# xStar_sq_DEM / yStar_sq_DEM
roots_per_kL <- 5.5
